{
   "version": "0.2.0",
   "configurations": [
      {
         "name": ".NET Core Launch (web)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build",
         "program": "${workspaceFolder}/bin/Debug/net5.0/Consultorio.dll",
         "args": [],
         "cwd": "${workspaceFolder}",
         "stopAtEntry": false,
         "console": "externalTerminal",
         "launchBrowser": {
            "enabled": false
         },

         /*"serverReadyAction": {
            "action": "openExternally",
            "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
         },*/

         "env": {
            "ASPNETCORE_ENVIRONMENT": "Development"
         },
         
         "sourceFileMap": {
            "/Views": "${workspaceFolder}/Views"
         }
      },
      {
         "name": ".NET Core Attach",
         "type": "coreclr",
         "request": "attach"
      }
   ]
}